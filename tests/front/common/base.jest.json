{
  "testPathIgnorePatterns": [
    "node_modules"
  ],
  "projects": [
    "<rootDir>/web/bundles"
  ],
  "moduleDirectories": [
    "node_modules",
    "web/bundles"
  ],
  "moduleFileExtensions": [
    "js",
    "jsx",
    "json",
    "node"
  ],
  "collectCoverage": true,
  "coverageReporters": [
    "html"
  ],
  "cacheDirectory": "/tmp/jest",
  "testURL": "http://localhost/",
  "coveragePathIgnorePatterns": ["./lib/"]
}
